{
  "name": "dev",
  "private": true,
  "scripts": {
    "start": "babel-node server.js",
    "prestart": "browserify src/index.js | uglifyjs > pub/bundle.js"
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "sassify": "^0.10.0",
    "stringify": "^5.1.0",
    "uglify-js": "^2.7.0",
    "emitonoff": "0.0.8",
    "express": "^4.13.4",
    "getbase": "^2.8.2",
    "highlight.js": "^9.4.0",
    "marked": "^0.3.5",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "engines": { "node": "0.12.7", "npm": "2.11.3" },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "browserify": {
    "transform": [
      "babelify",
      [
        "stringify",
        {
          "appliesTo": {
            "includeExtensions": [
              ".md",
              ".txt",
              ".htm",
              ".html"
            ]
          }
        }
      ],
      [
        "sassify",
        {
          "auto-inject": true
        }
      ]
    ]
  }
}
